{
  "name": "pokernight",
  "version": "1.0.0",
  "description": "poker app",
  "main": "index.js",
  "scripts": {
    "test": "npx jest",
    "test-watch": "npx jest --watchAll",
    "start": "npx ts-node -r tsconfig-paths/register server.ts",
    "start-watch": "npx nodemon -r tsconfig-paths/register server.ts",
    "export-types": "copy src\\types.ts ..\\client\\src\\schema\\",
    "build": "npx ttsc -P ./tsconfig.json",
    "build-prod": "npm install && npm run build && cd ../client && npm install && npm run build && mv build ../server/dist/",
    "build-prod:windows": "rmdir /s /q dist && npm install && npm run build && mkdir dist\\build && cd ..\\client && npm install && npm run build && xcopy /s /e build ..\\server\\dist\\build && rmdir /s /q build && cd ..\\server",
    "et": "npm run export-types",
    "sw": "npm run start-watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^6.0.0",
    "lodash": "^4.17.21",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.0",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.23",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.1",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "18.12.0"
  }
}
